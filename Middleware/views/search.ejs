
<div class="col-md-12">
<div class="row">
<div class="col-md-6 col-md-offset-5 " align="center;">
  <form action="/search" method="get">
	  <div class="input-group custom-search-input">
	    <input type="text" autocomplete="off" id="search" class="form-control input-lg" placeholder="Search Products Here" ng-model="q" ng-focus="search('focus')" ng-keyup="search('q')" name="search">
	    <!-- <input type="text" name="q" id="search" class="form-control search_input_text" style="margin-bottom:20px; font-size:18px" placeholder="Search by Restaurant"> -->

	    <div class="dropdown-toggle qsuggest"  data-toggle="dropdown" style="position:center; width: 500px; top: 32px; left: 0px;">
		  <ul class="dropdown-menu for-drop" style="width: 100%">
		  	<li ng-repeat="se in availableTags">
			  <a href="#" ng-click="userRedirect(se.p_id)" class="camel highlightstyle" style="display:table; padding: 0; width: 100%">
			  	<div class="col-xs-12 no-padding">
			  		<div class="col-xs-12" style="padding-top: 5px">
			  			<span class="suggest-name capitalize">{{ se.name }}</span>
			  		</div>
			  	</div>
			  </a>
			</li>
		  </ul>
	    </div>
	    <span class="input-group-btn ">
	      <button class="btn btn-info btn-lg" type="button" ng-click="searchItemRedirect()"><i class="glyphicon glyphicon-search"></i></button>
	    </span>

	  </div><!-- /input-group -->

  </form>
  </div>
</div>
</div>